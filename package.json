{
  "name": "react-embed",
  "version": "0.0.1",
  "description": "Embed formsort flows in react components",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "tslint --fix -p tsconfig.json",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formsort/react-embed.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "formsort",
    "embed",
    "react"
  ],
  "author": "Ali Kirecligol <alikirecligol@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/formsort/react-embed/issues"
  },
  "homepage": "https://github.com/formsort/react-embed#readme",
  "devDependencies": {
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.23",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "ts-jest": "^25.2.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.2.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@formsort/web-embed-api": "0.0.11"
  },
  "peerDependencies": {
    "react": "^16.13.0"
  }
}
